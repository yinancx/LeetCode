int lengthOfLongestSubstring(char* s) {
	int a[95], i = 0, length = 0, lgstlen = 0, tmp;
	
	for(int i = 0; i < 95; i++){
	    a[i] = 0;
	}
	for (i = 0; s[i] != 0 && lgstlen <= (strlen(s) - i) ; i++) {
	    tmp = i;
		while ( s[i] != 0 && a[ s[i] - ' ' ] != 1 ) {
			a[ s[i] - ' ' ] = 1;
			length++;
			i++;
// 		printf("%d a%d ",length,a[ s[i] - '!' ]);
		    
		}
		lgstlen = (length > lgstlen) ? length : lgstlen;
		length = 0;
		for(int i = 0; i < 95; i++){
	        a[i] = 0;
	    }
		i = tmp;
	}
	return lgstlen;
}
